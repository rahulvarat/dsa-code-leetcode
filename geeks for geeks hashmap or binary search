class Solution {
  public:
    int countFreq(vector<int>& arr, int target) {
        // code here
        unordered_map<int ,int >f;
        int count=0;
        for(int i=0;i<arr.size();i++){
            f[arr[i]]++;
            
        }
        for(auto i:f){
            int first=i.first;
            int second=i.second;
            if(target==first){
                return second;
                
            }
            
            
            

        }
        return 0;
    }
};
