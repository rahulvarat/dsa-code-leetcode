

class Solution {
public:
    ListNode* deleteMiddle(ListNode* head) {
        if(head==NULL || head->next==NULL){
            return NULL;
        }
        ListNode* dummy=new ListNode(0);
        dummy->next=head;
        ListNode* fast=head;
        ListNode*slow=dummy;
        while(fast!=NULL&&fast->next!=NULL){
            slow=slow->next;
            fast=fast->next->next;
        }
        ListNode* temp=slow->next;
        slow->next=temp->next;
        delete temp;
        return dummy->next;


    }
};
