class Solution {
public:
    int maxSubArray(vector<int>& arr) {
        int bestending=arr[0];
        int result=arr[0];
        for(int i=1;i<arr.size();i++){
            int v1=bestending+arr[i];
            int v2=arr[i];
            bestending=max(v1,v2);
            result=max(result,bestending);
        }
        return result;
    }
};
