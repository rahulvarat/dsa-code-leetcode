class Solution {
public:
    int findKthLargest(vector<int>& arr, int k) {
        priority_queue<int,vector<int>,greater<int>>pq;  //this line use for kth largest
        int i=0;
        for( i=0;i<k;i++){
            pq.push(arr[i]);
        }
        for(i=k;i<arr.size();i++){
          if(arr[i]>pq.top()){
            pq.pop();
            pq.push(arr[i]);
          }
        }
        return pq.top();
        
    }
};
